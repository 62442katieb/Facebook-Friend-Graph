{
  "name": "facebook-friend-graph",
  "version": "1.0.0",
  "description": "Create a graph of your facebook friends and the mutual connections between them",
  "main": "start.js",
  "scripts": {
    "build": "run-s clean compile package",
    "clean": "rimraf lib/ dist/",
    "compile": "run-p compile:*",
    "compile:core": "tsc -p ./src/core/tsconfig.json",
    "compile:service": "run-p compile:service:*",
    "compile:service:src": "tsc -p ./src/service/tsconfig.json",
    "precompile:service:views": "shx mkdir -p ./lib/service/",
    "compile:service:views": "shx cp -r ./src/service/views ./lib/service/",
    "compile:webapp": "tsc -p ./src/webapp/tsconfig.json",
    "predev": "run-s build",
    "dev": "run-p serve watch",
    "package": "rollup -c ./rollup.config.js",
    "serve": "node ./lib/service/bin/www.js",
    "start": "run-s build serve",
    "watch": "run-p watch:*",
    "watch:compile": "run-p watch:compile:*",
    "watch:compile:core": "npm run compile:core -- --watch",
    "watch:compile:service": "run-p watch:compile:service:*",
    "watch:compile:service:src": "npm run compile:service:src -- --watch",
    "watch:compile:webapp": "npm run compile:webapp -- --watch",
    "watch:package": "npm run package -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewiggins/Facebook-Friend-Graph.git"
  },
  "keywords": [
    "facebook",
    "graph"
  ],
  "author": "Andre Wiggins",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/andrewiggins/Facebook-Friend-Graph/issues"
  },
  "homepage": "https://github.com/andrewiggins/Facebook-Friend-Graph#readme",
  "dependencies": {
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "hbs": "^4.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lie": "^3.1.1",
    "preact": "^8.1.0"
  },
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/express": "^4.0.36",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-watch": "^4.0.0",
    "shx": "^0.2.2",
    "tslib": "^1.7.1",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  }
}
